{
  "name": "players-react-skeleton",
  "version": "1.0.0",
  "license": "MIT",
  "repository": "https://github.com/patemeryfl/players-react-skeleton",
  "scripts": {
    "start": "cross-env NODE_ENV=development parcel src/index.html -p 3000",
    "build": "cross-env NODE_ENV=production parcel build index.html --public-url",
    "e2e": "cypress open",
    "test": "jest",
    "lint:js": "eslint src/**/**/*.js src/**/**/*.jsx tests/**/**/*.js",
    "lint:style": "stylelint src/**/**/*.css src/**/**/*.scss"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/tests_enzyme/__tests__/setup/setupEnzyme.js",
    "testPathIgnorePatterns": [
      "<rootDir>/tests_enzyme/__tests__/setup/",
      "<rootDir>/tests/"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "ajv": "^6.5.2",
    "axios": "^0.18.0",
    "babel-eslint": "^8.2.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-react-app": "^3.1.1",
    "cross-env": "^5.2.0",
    "cypress": "^3.0.1",
    "jwt-decode": "^2.2.0",
    "node-sass": "^4.9.2",
    "parcel-bundler": "^1.9.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.4.0",
    "jest-enzyme": "^6.0.2",
    "prettier": "^1.13.5",
    "prettierrc": "0.0.0-5",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0"
  }
}
